{
  "name": "electro-test",
  "version": "1.0.0",
  "description": "",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack-dev": "webpack serve --mode development",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "dev": "concurrently \"npm run webpack-dev\" \"npm run electron-dev\"",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:icns": "bash tools/make-icns.sh app-resources/app-ico.png app-resources/app-ico.icns",
    "package:mac:prepare": "npm run build && npm run build:icns",
    "package:mac:pack": "npx --yes electron-packager . \"Podcast Creator\" --platform=darwin --out=release --overwrite --icon=app-resources/app-ico.icns --ignore='^/assets($|/)'",
    "package:mac": "npm run package:mac:prepare && npm run package:mac:pack -- --arch=arm64",
    "package:mac:x64": "npm run package:mac:prepare && npm run package:mac:pack -- --arch=x64",
    "package:mac:all": "npm run package:mac:prepare && npm run package:mac:pack -- --arch=arm64,x64"
  },
  "keywords": [],
